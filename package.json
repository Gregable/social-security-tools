{
  "name": "social-security-tools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --watch",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:ts": "prettier --write 'src/**/*.{ts,js,svelte}'",
    "quality": "npm run format:check && npm run lint && npm run check",
    "setup": "./scripts/setup.sh",
    "node:check": "node --version && echo 'Node.js version check passed'",
    "dev:safe": "npm run node:check && npm run dev",
    "build:safe": "npm run node:check && npm run build"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-mdx-gfm": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/svelte": "^8.6.14",
    "@storybook/sveltekit": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@sveltejs/adapter-vercel": "^5.7.2",
    "@sveltejs/kit": "^2.22.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-plugin-svelte": "^3.10.1",
    "posthog-js": "^1.77.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^8.6.14",
    "svelte": "^5.18.1",
    "svelte-check": "^4.2.1",
    "svelte-eslint-parser": "^1.2.0",
    "terser": "^5.43.1",
    "tslib": "^2.4.1",
    "typescript": "^5.3.3",
    "vite": "^7.1.6",
    "vitest": "^3.1.4"
  },
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "@rrweb/types": "^2.0.0-alpha.18"
  }
}
